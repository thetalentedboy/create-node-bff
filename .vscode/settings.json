{
    "eslint.enable": true,
    "eslint.validate": [
        "javascript"
    ],
    "prettier.enable": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.probe": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "vue",
        "markdown"
    ],
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
}